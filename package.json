{
    "name": "heex-html",
    "icon": "src/logo.png",
    "displayName": "Heex HTML",
    "description": "Syntax highlighting for HEEX templates in Phoenix Framework ðŸš€. Highlight Alpine JS , HEEX and Elixir template code",
    "version": "0.0.4",
    "engines": {
        "vscode": "^1.63.0"
    },
    "keywords": [
        "heex html",
        "heex",
        "phoenix html",
        "phoenix",
        "phoenix heex",
        "html heex",
        "elixir heex"
    ],
    "publisher": "ritvyk",
    "repository": {
        "url": "https://github.com/Ritvyk/vscode-heex-html"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "html",
            "aliases": ["html", "heex"],
            "extensions": [".html.heex"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "injectTo": [
                "text.html.heex",
                "text.heex",
                "text.ex"
            ],
            "embeddedLanguages": {
                "meta.embedded.block.html": "html",
               
                "meta.embedded.block.css": "css",
                "source.css": "css"
            },
            "language": "html",
            "scopeName": "text.html.heex",
            "path": "./syntaxes/html.tmLanguage.json"
        }]
    }
}